* {
  font-feature-settings: 'cv01', 'ss01', 'ss02', 'cv02' 0, 'cv03', 'cv05', 'cv06' 0, 'ss07' 0, 'cv07' 0, 'cv08', 'trad' 0, 'nlck', 'nalt' 0, 'ital', 'tnum' 0, 'ss03' 0, 'ss04';
}
:root {
  --monospace-font: SF Mono, monospace, ui-monospace;
  --xcode: XcodeDigits, SF Mono;
  --compact: SF Compact Text, SF Text;
}

.ͼ5 .cm-scroller,
.insertion-point-sidebar {
  --monospace-font-family: var(--monospace-font);
  --source-code-font-family: var(--monospace-font);
  font-family: var(--monospace-font) !important;
}

.ͼ5 .cm-lineNumbers,
.cm-gutters,
.cm-gutters * {
  --monospace-font-family: var(--xcode);
  --source-code-font-family: var(--xcode);
  font-family: var(--xcode) !important;
  font-feature-settings: 'tnum' !important;
  font-variant-numeric: tabular-nums !important;
}

.ͼ5 .cm-scroller {
  .cm-gutterElement {
    --monospace-font-family: var(--xcode);
    --source-code-font-family: var(--xcode);
    font-family: var(--xcode) !important;
  }
}

.call-frame-location {
  --monospace-font-family: var(--monospace-font);
  --source-code-font-family: var(--monospace-font);
  font-family: XcodeDigits, var(--monospace-font) !important;
}